<div id="dap-main" class="main">
    <!-- Everything must be inside a div so that the ejs works -->
    <div class="dashboard-templates">
        <div class="dap-footer-chart-stats" class="col-lg-12">
            <div class="dap-table-graph-footer">
                <div class="dap-table-title"></div>
                <div class="dap-table-evens-occurrence dap-prediction-table">
                    <table class="table table-condensed dap-table-evens-occurrence">
                        <thead>
                            <tr>
                                <th colspan="2">Events</th>
                            </tr>
                            <tr>
                                <th>Event</th>
                                <th>Timestamp</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr class="dashboard-templates">
                                <td class="dap-td-1"></td><td class="dap-td-2"></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
                <div class="dap-table-evens-prediction dap-prediction-table">
                    <table class="table table-condensed">
                        <thead>
                            <tr>
                                <th colspan="2">Event predictions</th>
                            </tr>
                            <tr>
                                <th>Event</th>
                                <th>Prediction</th>
                            </tr>
                        </thead>
                        <tbody>
                        <tr class="dashboard-templates">
                            <td class="dap-td-1"></td><td class="dap-td-2"></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="dap-table-evens-prediction-result dap-prediction-table">
                    <table class="table table-condensed">
                        <thead>
                        <tr>
                            <th colspan="3">
                                Prediction results
                                <span style="float: right">
                                <button class="btn btn-default footer-button" onclick="cleanPredictionResults()"><span class="glyphicon glyphicon-trash"></span></button>
                                    </span>
                            </th>


                        </tr>
                        <tr>
                            <th>Event</th>
                            <th>Timestamp</th>
                            <th>Prediction result</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr class="dashboard-templates">
                            <td class="dap-td-1"></td><td class="dap-td-2"></td><td class="dap-td-3"></td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="dap-table-evens-alerts dap-prediction-table">
                    <table class="table table-condensed">
                        <thead>
                        <tr>
                            <th colspan="2">
                                Alerts
                            </th>


                        </tr>
                        <tr>
                            <th>Event</th>
                            <th>Alert Type</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr class="dashboard-templates">
                            <td class="dap-td-1"></td>
                            <td class="dap-td-2"></td>
