<div id="dd-main" class="main">
    <div class="row" id="source-row">
        <div class="col-lg-4">
            <h1 class="page-header">Dynamic population distribution</h1>
        </div>
        <div id="dd-dropArea" class="col-lg-2 dropArea">
            Drop Here<br>
            <button class="btn btn-default" id="dd-upload-button" onclick="upload()">Upload</button>
            <button class="btn btn-default" onclick="reset()">Reset</button>


        </div>
        <div class="col-lg-5">
            <div class="navbar-left col-xs-12 col-sm-12 col-md-12 col-lg-12" role="search">
                <form class="navbar-left" id="dd-upload" role="search">
                    <input id="dd-input-button" type="file" name="upl" multiple style="display: none"/>
                </form>
                <div class="panel panel-default ">
                    <div class="panel-heading">
                        <span style="font-weight: bold">Data bases</span>
                    </div>
                    <div class="panel-body">
                        <div class="dd-database-entry col-xs-12 col-sm-12 col-md-12 col-lg-12" id="dd-databases">
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <div class="row" id="dd-attributes-row">

        <div class="col-lg-9">
            <div class="row col-lg-12">
                <div class="col-lg-4">
                    <div class="input-group input-group-sm" id="group-select-population-col">
                        <div id="dd-select-population"/>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="input-group input-group-sm" id="group-select-category-col" >
                        <div id="dd-select-category-col"/>
                    </div>
                </div>
                <div class="col-lg-4">
                    <div class="input-group input-group-sm" id="group-select-category-col" >
                        <div id="dd-select-secondary-col"/>
                    </div>
                </div>
            </div>
            <div id="dd-graphics-row" class="row col-lg-12">
                <div id="dynamic-distribution-chart-div">
                </div>
            </div>
        </div>
        <div class="col-lg-3" id="dd-filters-row">
            <div id="dd-filter-population">
            </div>
            <div id="dd-filter-category">
            </div>
            <div id="dd-filter-secondary">
            </div>
        </div>


    </div>

    <script>
        preLoad();

        if (dynamicDistributionObject !== undefined) {
            reset();
        }
        postLoad();
        // Preload




    </script>
</div>